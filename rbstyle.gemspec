Gem::Specification.new do |spec|
    spec.name          = "rbstyle"
    spec.version       = "0.1.0"
    spec.authors       = ["Codepulze"]
    spec.summary       = "Console Application Styler"
    spec.description   = "Make eye catching console applications in ruby, check https://github.com/EvilBytecode/rbstyle for more info."
